default: python /usr/local/quast/quast.py $LABELS $REFERENCES --threads `nproc` --output-dir ${WORK_DIR}/combined_quast_output $CONTIGS  
metaquast-default: python /usr/local/quast/metaquast.py $LABELS $REFERENCES --threads `nproc` --output-dir $WORK_DIR $CONTIGS  
metaquast-scaffold: python /usr/local/quast/metaquast.py $LABELS $REFERENCES --threads `nproc` --scaffolds --output-dir $WORK_DIR $CONTIGS  
